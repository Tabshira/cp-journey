//Sieve of Eratosthenes (check prime)
  vector<bool> isPrime(n+1,1);
  isPrime[0]=isPrime[1]=0;
  for(int i=2;i*i<=n;i++)
  {
      if(isPrime[i]==1)
      {
          for(int j=i*i;j<=n;j+=i)
          {
              isPrime[j]=0;
          }
      }
  }

//NOD Sieve
  vector<int> divisors(n+1,0);
  for(int i=1;i<=n;i++)
  {
    for(int j=i;j<=n;j+=i)
          {
              divisors[j]++;
          }
  }
